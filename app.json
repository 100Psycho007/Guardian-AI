{
  "expo": {
    "name": "Expo Router Starter",
    "slug": "expo-router-starter",
    "version": "1.0.0",
    "scheme": "exporouterstarter",
    "entryPoint": "./index.js",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.DETECT_SCREEN_CAPTURE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ]
  }
}
